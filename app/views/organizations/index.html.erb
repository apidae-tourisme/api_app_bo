<p id="notice"><%= notice %></p>

<h1>Listing Organizations</h1>

<table>
  <thead>
    <tr>
      <th>Created at</th>
      <th>Updated at</th>
      <th>Name</th>
      <th>Description</th>
      <th>Address</th>
      <th>Email</th>
      <th>Telephone</th>
      <th>Thumbnail</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organizations.each do |organization| %>
      <tr>
        <td><%= organization.created_at %></td>
        <td><%= organization.updated_at %></td>
        <td><%= organization.name %></td>
        <td><%= organization.description %></td>
        <td><%= organization.address %></td>
        <td><%= organization.email %></td>
        <td><%= organization.telephone %></td>
        <td><%= organization.thumbnail %></td>
        <td><%= organization.latitude %></td>
        <td><%= organization.longitude %></td>
        <td><%= link_to 'Show', organization %></td>
        <td><%= link_to 'Edit', edit_organization_path(organization) %></td>
        <td><%= link_to 'Destroy', organization, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Organization', new_organization_path %>
